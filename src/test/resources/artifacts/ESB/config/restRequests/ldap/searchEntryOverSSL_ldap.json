{
  "providerUrl": "%s(providerSSLUrl)",
  "securityPrincipal": "%s(securityPrincipal)",
  "securityCredentials": "%s(securityCredentials)",
  "secureConnection": "true",
  "disableSSLCertificateChecking": "true",
  "application": "ldap",
  "operation": "searchEntity",
  "onlyOneReference":"%s(onlyOneReference)",
  "content": {
    "dn": "%s(ldapUserBase)",
    "objectClass": "inetOrgPerson",
    "attributes": "mail,uid,givenName",
    "filters": {
      "uid": "%s(testUserId)"
    }
  }
}